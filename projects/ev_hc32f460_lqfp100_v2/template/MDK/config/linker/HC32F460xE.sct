LR_IROM1 0x00000000 0x80000  {    ; load region size_region
    ER_IROM1 0x00000000 0x80000  {  ; load address = execution address
        *.o (RESET, +First)
        *(InRoot$$Sections)
        .ANY (+RO)
        .ANY (+XO)
    }
    RW_IRAM1 0x1FFF8000 UNINIT 0x00000008  {  ; RW data
        *(.bss.noinit)
    }
    RW_IRAM2 0x1FFF8008 0x0002EFF8  {  ; RW data
        .ANY (+RW +ZI)
        .ANY (RAMCODE)
    }
    RW_IRAMB 0x200F0000 0x00001000  {  ; RW data
        .ANY (+RW +ZI)
    }
}

EX_QSPI_ROM 0x98000000 0x800000  {
    QSPI_ROM 0x98000000 0x800000  {
        *.o (.ex_rom)
		;lv_*.o (+RO)
		;lv_demo_music*.o (+RO)
		lv_font*.o (+RO)
